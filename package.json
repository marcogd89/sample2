{
  "name": "CB-Slack-Notify",
  "version": "0.0.1",
  "description": "CB Notifier",
  "private": true,
  "author": "Marco Daniele",
  "main": "dist/server.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "start": "node dist/server.js",
    "dev": "nodemon src/server.js --exec \"node -r babel-register\" | pino-pretty",
    "clean": "rimraf dist",
    "build": "npm run clean && mkdir dist && babel src -s -d dist",
    "prestart": "npm run build",
    "debug": "node --debug-brk --inspect ./node_modules/jest/bin/jest -i",
    "precommit": "npx pretty-quick --staged",
    "coverage": "npm test -- --coverage",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/*.js",
    "format": "eslint src/*.js --fix"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "license": "ISC",
  "dependencies": {
    "@azure/cosmos": "^3.6.2",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "crypto-js": "^4.0.0",
    "debug": "^4.1.1",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "lodash": "^4.17.15",
    "morgan": "^1.9.1",
    "nock": "^11.3.5",
    "pino": "^5.13.3",
    "pino-pretty": "^3.6.1",
    "request": "^2.88.0",
    "request-promise": "^4.2.4"
  },
  "devDependencies": {
    "axios": "^0.21.2",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^23.6.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "eslint": "^6.4.0",
    "eslint-config-google": "^0.13.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-react": "^7.14.3",
    "husky": "^3.0.5",
    "jest": "^23.6.0",
    "lint-staged": "^9.2.5",
    "moxios": "^0.4.0",
    "nodemon": "^1.18.7",
    "prettier": "^1.15.2",
    "pretty-quick": "^1.8.0",
    "rimraf": "^2.6.2",
    "sinon": "^7.4.2",
    "supertest": "^3.3.0"
  },
  "engines": {
    "node": "~10.x.x",
    "npm": "~6.x.x"
  },
  "jest": {
    "testEnvironment": "node",
    "verbose": true,
    "coverageReporters": [
      "json",
      "html"
    ]
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ],
    "plugins": [
      "transform-object-rest-spread",
      "transform-class-properties"
    ],
    "ignore": [
      "**/__mocks__",
      "**/*.test.js"
    ]
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  }
}
